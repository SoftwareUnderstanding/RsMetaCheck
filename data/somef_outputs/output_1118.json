on"}], "keywords": [{"result": {"value": ["Alpino", "Dependency parsing", "SPOD: Syntactic profiler of Dutch", "XPath", "UD: Universal Dependencies"], "type": "String"}, "confidence": 1, "technique": "code_parser", "source": "https://raw.githubusercontent.com/rug-compling/paqu/master/codemeta.json"}], "citation": [{"result": {"value": "The Parse and Query (PaQu) Application", "title": "The Parse and Query (PaQu) Application", "type": "Scholarly_article", "url": "https://doi.org/10.5334/bbi.23"}, "confidence": 1, "technique": "code_parser", "source": "https://raw.githubusercontent.com/rug-compling/paqu/master/codemeta.json"}], "development_status": [{"result": {"value": ["https://www.repostatus.org/#active", "https://w3id.org/research-technology-readiness-levels#Stage4Complete"], "type": "String"}, "confidence": 1, "technique": "code_parser", "source": "https://raw.githubusercontent.com/rug-compling/paqu/master/codemeta.json"}], "readme_url": [{"result": {"value": "https://github.com/rug-compling/paqu/blob/master/README.md", "type": "String"}, "confidence": 1, "technique": "code_parser", "source": "https://raw.githubusercontent.com/rug-compling/paqu/master/codemeta.json"}, {"result": {"value": "https://raw.githubusercontent.com/rug-compling/paqu/master/README.md", "type": "Url"}, "confidence": 1, "technique": "file_exploration"}], "version": [{"result": {"value": "1.0.8", "type": "String"}, "confidence": 1, "technique": "code_parser", "source": "https://raw.githubusercontent.com/rug-compling/paqu/master/codemeta.json"}], "authors": [{"result": {"value": "Peter Kleiweg", "type": "String", "email": "mailto:p.c.j.kleiweg@rug.nl"}, "confidence": 1, "technique": "code_parser", "source": "https://raw.githubusercontent.com/rug-compling/paqu/master/codemeta.json"}], "license": [{"result": {"value": "https://spdx.org/licenses/BSD-2-Clause", "type": "License", "identifier": "https://spdx.org/licenses/https://spdx.org/licenses/BSD-2-Clause", "spdx_id": "https://spdx.org/licenses/BSD-2-Clause"}, "confidence": 1, "technique": "code_parser", "source": "https://raw.githubusercontent.com/rug-compling/paqu/master/codemeta.json"}], "requirements": [{"result": {"value": "https://github.com/rug-compling/paqu#benodigde-software", "url": "https://github.com/rug-compling/paqu#benodigde-software", "type": "Software_application"}, "confidence": 1, "technique": "code_parser", "source": "https://raw.githubusercontent.com/rug-compling/paqu/master/codemeta.json"}, {"result": {"value": "Om PaQu te draaien heb je van\n[Alpino](http://www.let.rug.nl/vannoord/alp/Alpino/) de tokenizer en de\nparser nodig.\n\nJe kunt ook een Alpino-server gebruiken die voldoet aan de\n[Alpino API](https://github.com/rug-compling/alpino-api).\nAls je dat doet heb je zelf van Alpino alleen de tokenizer nodig.\nZo'n server kan in principe de data parallel verwerken, en dus veel\nsneller zijn dan wanneer je Alpino lokaal gebruikt.\n\n----------------------------------------------------------------\n", "type": "Text_excerpt", "original_header": "Alpino: A dependency parser for Dutch", "parent_header": ["PaQu", "Benodigde software"]}, "confidence": 1, "technique": "header_analysis", "source": "https://github.com/rug-compling/paqu/blob/master/README.md"}], "has_script_file": [{"result": {"value": "https://raw.githubusercontent.com/rug-compling/paqu/master/run/run.sh", "type": "Url"}, "confidence": 1, "technique": "file_exploration"}], "usage": [{"result": {"value": "De meeste programma's zijn geschreven in de programmeertaal Go. Je hebt\ndus de Go-compiler nodig.\n\nJe hebt minimaal Go versie 1.8 nodig. Geef het commando `go version` om\ndit te controleren.\n\nJe kunt Go downloaden van http://golang.org/\n", "type": "Text_excerpt", "original_header": "Go: The Go Programming Language", "parent_header": ["PaQu", "Benodigde software"]}, "confidence": 1, "technique": "header_analysis", "source": "https://github.com/rug-compling/paqu/blob/master/README.md"}], "full_title": [{"result": {"ty